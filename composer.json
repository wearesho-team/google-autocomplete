{
  "name": "wearesho-team/google-autocomplete",
  "description": "Service for searching cities and streets using passed input value",
  "type": "library",
  "authors": [
    {
      "name": "Beton",
      "email": "aleksa.yagich@gmail.com"
    },
    {
      "name": "Roman Varkuta",
      "email": "roman.varkuta@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Wearesho\\GoogleAutocomplete\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\GoogleAutocomplete\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">7.1",
    "horat1us/environment-config": "^1.2",
    "guzzlehttp/guzzle": "^6.3",
    "myclabs/php-enum": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "squizlabs/php_codesniffer": "^3.2",
    "yiisoft/yii2": "2.0.15.1"
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  },
  "suggests": {
    "yiisoft/yii2": "If you want to configure your yii application easily with bootstrap"
  }
}
